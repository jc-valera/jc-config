[alias]
	st = status
	ci = commit -m
	co = checkout
	br = branch
	di = diff
	dv = difftool -t vimdiff -y
	last = log -1 HEAD --stat
	logTree = log --graph --decorate --all	
	alias = ! git config --get-regexp ^alias\\. | sed -e s/^alias\\.// -e s/\\ /\\ =\\ /
	
